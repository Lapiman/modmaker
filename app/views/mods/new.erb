<h1>New Mod</h1>

<%= form_for :mod, url: mods_path do |f| %>
  <p>
    <%= f.label :modid %><br>
    <%= f.text_field :modid %>
  </p>
  
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  
  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>